<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>

<?import javafx.scene.layout.VBox?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ListView?>
<AnchorPane fx:id="anchor" prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.view.controller.SettingWindow">
   <children>
      <GridPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columnConstraints>
            <ColumnConstraints hgrow="ALWAYS" />
            <ColumnConstraints prefWidth="120" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints percentHeight="100" />
        </rowConstraints>
         <children>
             <ListView />
                 <VBox GridPane.columnIndex="1"  alignment="TOP_CENTER" spacing="8.0">
                     <children>
                         <Button text="Add"  maxWidth="Infinity"/>
                         <Button text="Edit" maxWidth="Infinity"/>
                         <Button text="Delete" maxWidth="Infinity"/>
                     </children>
                     <padding>
                         <Insets bottom="8" top="8" left="8" right="8"/>
                     </padding>
                 </VBox>
         </children>
      </GridPane>
   </children>
</AnchorPane>
